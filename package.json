{
  "name": "devices-flow",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist && rimraf projects/demo/src/assets/devices-flow.lcu.js",
    "build": "npm run build:common && npm run build:lcu && npm run build:demo",
    "build:common": "ng build --project common",
    "build:demo": "ng build --project demo",
    "build:lcu": "ng build --extra-webpack-config projects/lcu/webpack.externals.js --project lcu --single-bundle && npm run pack",
    "deploy": "npm version patch && npm run deploy:all",
    "deploy:all": "npm run deploy:common && npm run deploy:lcu && npm run deploy:demo",
    "deploy:common": "npm version patch --prefix projects/common && npm run build:common && npm publish ./dist/common --access public",
    "deploy:demo": "npm version patch --prefix projects/demo && npm run build:demo && npm publish ./dist/demo --access public",
    "deploy:lcu": "npm version patch --prefix projects/lcu && npm run build:lcu && npm publish ./dist/lcu --access public",
    "move:lcu": "cp dist/lcu/wc projects/demo/src/assets",
    "pack": "npm run pack:lcu",
    "pack:join": "concat-glob-cli -f \"dist/lcu/wc/lcu.*.js\" -o dist/lcu/wc/devices-flow.lcu.js",
    "pack:lcu": "rimraf dist/lcu/wc && mkdirp dist/lcu/wc && npm run pack:main && npm run pack:pollyfills && npm run pack:join && npm run pack:styles",
    "pack:main": "concat-glob-cli -f \"dist/lcu/main.*.js\" -o dist/lcu/wc/lcu.startup.js",
    "pack:pollyfills": "concat-glob-cli -f \"dist/lcu/scripts.*.js\" -o dist/lcu/wc/lcu.pollyfills.js",
    "pack:styles": "concat-glob-cli -f \"dist/lcu/wc/styles.*.css\" -o dist/lcu/wc/devices-flow.lcu.css",
    "start": "npm run clean && npm-run-all --parallel watch:*",
    "start:common": "ng build --project common --watch --configuration development",
    "start:demo": "ng serve demo --port=4200 --aot=false",
    "start:lcu": "ng build --extra-webpack-config projects/lcu/webpack.externals.js --project lcu --single-bundle --watch --configuration development",
    "watch:common": "ng build common --watch",
    "watch:demo": "wait-on dist/common/public-api.d.ts && npm run start:demo",
    "test": "ng test",
    "postinstall:bak": "",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.4",
    "@angular/cdk": "^12.0.5",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/elements": "^12.0.4",
    "@angular/flex-layout": "^12.0.0-beta.34",
    "@angular/forms": "^12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "@aspnet/signalr": "^1.0.27",
    "@lcu/common": "^1.84.735",
    "@lowcodeunit-devkit/ng-lcu": "^1.28.161",
    "@lowcodeunit/app-host-common": "1.20.132",
    "@lowcodeunit/lazy-element": "^1.20.64",
    "@lowcodeunit/data-grid": "^1.20.236",
    "@lowcodeunit/lcu-documentation-common": "^1.6.38",
    "@lowcodeunit/lcu-icons-common": "^1.2.56",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "ngx-build-plus": "^12.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.4",
    "@angular/cli": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "concat-glob-cli": "^0.1.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~4.2.3",
    "wait-on": "^5.3.0"
  }
}
